{
  "type": "<%= project.type %>",
  "name": "<%= project.package_name %>",
  "version": "1.0.0",
  "displayName": "<%= project.display_name %>",
  "description": "<%= project.description %>",
  "dependencies": {
  },
  "keywords": [
    <%- project.keywords.each do |word| -%>
    "<%= word %>",
    <%- end -%>
    "keyword"
  ],
  "author": {
    "name": "<%= project.author_name %>",
    "email": "<%= project.author_email %>",
    "url": "<%= project.author_url %>"
  },
  "git": {
    "url": "<%= project.git_url %>",
    "tag": "<%= project.git_release_tag %>"
  },
  <%- case project.type -%>
  <%- when "unity" -%>
  "unity": {
    "version": "<%= project.unity_version %>",
    "release": "<%= project.unity_release %>"
  }
  <%- else -%>
  <%- end -%>
}